        "-Command Start-Process",
        "-Wait",
        "-UseNewEnvironment",
        "-NoNewWindow",
        "-FilePath",
        "'${env.GBS_BIN}'",
        "-ArgumentList",
        "'cl=msvc config=release,warnings build'"
      ]
    },
