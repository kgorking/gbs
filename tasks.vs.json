{
  "version": "0.2.1",
  "outDir": "\"${workspaceRoot}\\gbs.out\"",
  "tasks": [
    {
      "taskLabel": "Enumerate compilers",
      "appliesTo": "/*/",
      "type": "launch",
      "contextType": "custom",
      "workingDirectory": "${file}",
      "command": "pwsh",
      "args": [
        "-Command Start-Process",
        "-Wait",
        "-UseNewEnvironment",
        "-NoNewWindow",
        "-FilePath",
        "'${env.GBS_BIN}'",
        "-ArgumentList",
        "'enum_cl'"
      ]
    },
    {
      "taskLabel": "unittests",
      "appliesTo": "/*/",
      "type": "launch",
      "contextType": "custom",
      "workingDirectory": "${file}",
      "command": "pwsh",
      "args": [
        "-Command Start-Process",
        "-Wait",
        "-UseNewEnvironment",
        "-NoNewWindow",
        "-FilePath",
        "'${env.GBS_BIN}'",
        "-ArgumentList",
        "'unittest'"
      ]
    },
