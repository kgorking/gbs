{
  "version": "0.2.1",
  "defaults": {},
  "configurations": [
    {
      "type": "default",
      "project": "CMakeLists.txt",
      "projectTarget": "gbs.exe",
      "currentDir": ".",
      "name": "gbs.exe"
    },
    {
      "name": "Unittest sample project",
      "type": "default",
      "project": "CMakeLists.txt",
      "currentDir": "./sample_project_all/",
      "projectTarget": "gbs.exe",
      "args": [
        "cl=msvc config=debug,warnings,analyze build unittest"
      ]
    },
    {
      "name": "version",
      "type": "default",
      "project": "CMakeLists.txt",
      "currentDir": ".",
      "projectTarget": "gbs.exe",
      "args": [
        "version"
      ]
    },
    {
      "name": "get_cl",
      "type": "default",
      "project": "CMakeLists.txt",
      "currentDir": ".",
      "projectTarget": "gbs.exe",
      "args": [
        "get_cl"
      ]
    },
    {
      "name": "Enumerate compilers",
      "type": "default",
      "project": "CMakeLists.txt",
      "currentDir": ".",
      "projectTarget": "gbs.exe",
      "args": [
        "enum_cl"
      ]
    },
    {
      "name": "[msvc] build",
      "type": "default",
      "project": "CMakeLists.txt",
      "currentDir": ".",
      "projectTarget": "gbs.exe",
      "args": [
        "clean cl=msvc build unittest run=version"
      ]
    },
    {
      "name": "[clang] build",
      "type": "default",
      "project": "CMakeLists.txt",
      "currentDir": ".",
      "projectTarget": "gbs.exe",
      "args": [
        "clean cl=clang build unittest run=version"
      ]
    },
    {
      "name": "[clang] build for linux",
      "type": "default",
      "project": "CMakeLists.txt",
      "currentDir": ".",
      "projectTarget": "gbs.exe",
      "args": [
        "clean cl=clang:22 build unittest run=version"
      ]
    },
    {
      "name": "[gcc] build",
      "type": "default",
      "project": "CMakeLists.txt",
      "currentDir": ".",
      "projectTarget": "gbs.exe",
      "args": [
        "clean cl=gcc build unittest run=version"
      ]
    },
    {
      "name": "Download latest gcc + clang",
      "type": "default",
      "project": "CMakeLists.txt",
      "currentDir": ".",
      "projectTarget": "gbs.exe",
      "args": [
        "get_cl=clang get_cl=gcc"
      ]
    },
    {
      "name": "[clang] self analyze",
      "type": "default",
      "project": "CMakeLists.txt",
      "currentDir": ".",
      "projectTarget": "gbs.exe",
      "args": [
        "clean cl=clang config=debug,analyze,warnings build"
      ]
    },
    {
      "name": "[msvc] self analyze",
      "type": "default",
      "project": "CMakeLists.txt",
      "currentDir": ".",
      "projectTarget": "gbs.exe",
      "args": [
        "clean cl=msvc config=debug,analyze,warnings build"
      ]
    }
  ]
}